		start_event = { trigger_once = yes sound = { soundeffect = "fallen_empire_colossus_energy_field" } }
		
		event = { time = 0 node = "planet_killer_gun_01" particle = "fallen_empire_02_colossus_core_effect" keep_particle = yes trigger_once = yes }
	}

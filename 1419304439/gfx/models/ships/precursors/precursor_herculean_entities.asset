	}
	# Moving state if going from open/working, uses closing animation.
	state = { name = "moving" animation = "closing" looping = no next_state = "moving"
		chance = {
			if_current_state = {
				"idle" = 0
				"moving" = 0
				"death" = 0
				"working" = 0
				"working_looping" = 100
			}
		}
		event = { trigger_once = yes sound = { soundeffect = "fallen_empire_colossus_closing" stop_on_state_change = yes } }
		
		
		event = { time = 0 node = "planet_killer_gun_01" particle = "fallen_empire_02_colossus_core_effect" keep_particle = yes trigger_once = yes }
	
		event = { time = 0 node = "root" particle = "fallen_empire_02_colossus_energy_start_effect" keep_particle = yes trigger_once = yes }
	}
	# Opened state, going after "working".
	state = { name = "working_looping" animation = "open" 
		event = { time = 0	id = "beam_start" }
	
		
		event = { time = 0 node = "planet_killer_gun_01" particle = "fallen_empire_02_colossus_core_effect" keep_particle = yes trigger_once = yes }
			
		event = { time = 0 node = "large_shield_mid_effect_1" particle = "fallen_empire_02_colossus_shield_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "large_shield_mid_effect_2" particle = "fallen_empire_02_colossus_shield_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "large_shield_mid_effect_3" particle = "fallen_empire_02_colossus_shield_effect" keep_particle = yes trigger_once = yes }
	
	}
	# Opening state, goes into "working_looping" state when done.
	state = { name = "working" animation = "opening" looping = no next_state = "working_looping" 
